    "commander": "^10.0.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
